<!DOCTYPE html>
<html>
    <head>
      <link href="../css/cpu.css" rel="stylesheet" type="text/css" media="all">
    </head>
    <body>
        <h1 class="teacher">School as a CPU : {{sGroupName}}</h1>
        
        {{#aWorksheets}}
        <div class="teacher">
            <h2>Worksheet ID : {{oLevel.sGrade}}/{{iSheetNumber}}</h2>
            
            <table class="student">
                <tr><th>Student Name</th><td>&nbsp;</td></tr>
            </table>                
            
            <br />
            <table>
                <tr>
                    <th>MAR</th>
                    <th>Physical Location</th>
                    <th>Data / MDR</th>
                    {{#if oLevel.gradeB}}
                        <th>CIR / Operation</th>
                        <th>Operand</th>
                    {{else}}
                        <th>Decoded</th>
                    {{/if}}
                    <th>ACC</th>
                    <th>Program Counter</th>
                </tr>
                {{#aChosen}}<tr>
                    <td>{{iStep}}</td>
                    <td>{{sRoomName}}</td>
                    <td>{{sData}}</td>
                    {{#if ../oLevel.gradeB}}
                        <td>{{#if bListOperation}}{{operation}}{{else}}&nbsp;{{/if}}</td>
                        <td>{{#if bListOperand}}{{operand}}{{else}}&nbsp;{{/if}}</td>
                    {{else}}
                        <td>{{sDisplay}}</td>
                    {{/if}}
                    <td>{{iResult}}</td>
                    <td>{{iNextStep}}</td>
                </tr>{{/aChosen}}
            </table>
        </div>
        {{/aWorksheets}}
    </body>
</html>
